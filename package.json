{
  "name": "Anular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel back front",
    "back": "cd server && npx json-server db.json -m ./node_modules/json-server-auth",
    "front": "cd project && ng serve",
    "build": "cd project && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:project": "node dist/project/server/server.mjs"
  }
}
